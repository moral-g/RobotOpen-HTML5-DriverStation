{
  "name": "RobotOpen DS 708",
  "description": "Forked and maintained Robot Open Driver Station",
  "version": "1.0.0",
  "manifest_version": 3,
  "minimum_chrome_version": "24",

  "permissions": [
    {"socket": [
      "udp-send-to",
      "udp-bind"
    ]},
    "notifications",
    "storage"
  ],

  "app": {
    "background": {
      "scripts": ["chromeapp.js"]
    }
  },

  "icons": { 
    "128": "logo-128.png",
    "48": "logo-48.png",
    "16": "logo-16.png"
  }
  
}
